# Cache images, icons, and media for one month
<FilesMatch "\.(jpg|jpeg|png|gif|ico|webp|svg|woff|woff2|ttf|otf)$">
  Header set Cache-Control "max-age=2592000, public"
</FilesMatch>

# Cache CSS and JavaScript for one week
<FilesMatch "\.(css|js)$">
  Header set Cache-Control "max-age=604800, public"
</FilesMatch>

